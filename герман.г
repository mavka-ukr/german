  СЛОВО_ФРАГМЕНТ: 'ф' 'р' 'а' 'г' 'м' 'е' 'н' 'т';
  ВІДКРИТА_ДУЖКА: '(';
  ЗАКРИТА_ДУЖКА: ')';
  ЗІРОЧКА: '*';
  ПЛЮС: '+';
  ДВОКРАПКА: ':';
  КРАПКАКОМА: ';';
  РІВНО: '=';
  ЗНАК_ПИТАННЯ: '?';
  ВІДКРИТА_ФІГУРНА_ДУЖКА: '{';
